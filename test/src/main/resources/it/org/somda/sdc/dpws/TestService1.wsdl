                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Param1" type="xsd:string"/>
                        <xsd:element name="Param2" type="xsd:int"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="TestOperationResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Result1" type="xsd:string"/>
                        <xsd:element name="Result2" type="xsd:int"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="TestNotification">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Param1" type="xsd:string"/>
                        <xsd:element name="Param2" type="xsd:int"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="TestOperationRequest">
        <wsdl:part name="parameters" element="msg:TestOperationRequest"/>
    </wsdl:message>
    <wsdl:message name="TestOperationResponse">
        <wsdl:part name="parameters" element="msg:TestOperationResponse"/>
    </wsdl:message>
    <wsdl:message name="TestNotification">
        <wsdl:part name="parameters" element="msg:TestNotification"/>
    </wsdl:message>
    <wsdl:portType name="TestPortType1" wse:EventSource="true" dpws:DiscoveryType="tns:DpwsTest">
        <wsdl:operation name="TestOperation">
            <wsdl:input message="tns:TestOperationRequest"/>
            <wsdl:output message="tns:TestOperationResponse"/>
        </wsdl:operation>
        <wsdl:operation name="TestNotification">
            <wsdl:output message="tns:TestNotification"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:portType name="TestPortType2" wse:EventSource="true" dpws:DiscoveryType="tns:DpwsTest">
        <wsdl:operation name="TestOperation">
            <wsdl:input message="tns:TestOperationRequest"/>
            <wsdl:output message="tns:TestOperationResponse"/>
        </wsdl:operation>
        <wsdl:operation name="TestNotification">
            <wsdl:output message="tns:TestNotification"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="TestBinding1" type="tns:TestPortType1">
        <wsp:Policy>
            <dpws:Profile wsp:Optional="true"/>
        </wsp:Policy>
        <s12:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="TestOperation">
            <s12:operation soapAction="http://dpws-test-service/2017/05/10/TestPortType1/TestOperation"/>
            <wsdl:input>
                <s12:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <s12:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
        <wsdl:operation name="TestNotification">
            <s12:operation soapAction="http://dpws-test-service/2017/05/10/TestPortType1/TestNotification"/>
            <wsdl:output>
                <s12:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:binding name="TestBinding2" type="tns:TestPortType2">
        <wsp:Policy>
            <dpws:Profile wsp:Optional="true"/>
        </wsp:Policy>
        <s12:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
